{
  "name": "metalsmith-concat",
  "version": "4.2.1",
  "main": "lib",
  "description": "A Metalsmith plugin to concatenate files",
  "author": "Aymeric Beaumet <aymeric@beaumet.me>",
  "license": "CC0-1.0",
  "keywords": [
    "metalsmith",
    "plugin"
  ],
  "scripts": {
    "test": "standard && istanbul test node_modules/tape/bin/tape -- 'test/**/*.js'",
    "test-coverage": "npm test --coverage && istanbul check-coverage --statements 100 --functions 100 --branches 100 --lines 100",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "dependencies": {
    "minimatch": "3.0.0"
  },
  "devDependencies": {
    "codeclimate-test-reporter": "0.3.1",
    "istanbul": "0.4.2",
    "standard": "6.0.8",
    "tape": "4.5.1"
  }
}
